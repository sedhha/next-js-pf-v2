{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["components/*"],
			"@/pages/*": ["pages/*"],
			"@/styles/*": ["styles/*"],
			"@/constants/*": ["constants/*"],
			"@/slices/*": ["redux/slices/*"],
			"@/redux/*": ["redux/tools/*"],
			"@/utils/*": ["utils/*"],
			"@/v2/*": ["components/v2/*"],
			"@/interfaces/*": ["interfaces/*"],
			"@/hooks/*": ["hooks/*"],
			"@/firebase/*": ["backend/firebase/*"],
			"@/appscript/*": ["backend/appscript/*"],
			"@/fe-client/*": ["utils/fe/apis/services/*"],
			"@/middleware/*": ["middleware/*"],
			"@/backend/*": ["backend/*"],
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "types.d.ts", "backend/security/index.js"],
	"exclude": ["node_modules"]
}
